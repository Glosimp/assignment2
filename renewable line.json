{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Renewable Energy from Each state",
    "width":400,
    "height":300,
    "description": "Renewable Energy from Each state.",
    "data": {"url": "https://raw.githubusercontent.com/Glosimp/assignment2/main/LINE%20O%20OWEN%201.csv"},
     "params": [
      {
        "name": "Fuel_selection",
        "bind": {
          "input": "select",
          "options": [
            "Bagasse",
            "Biogas",
            "Wind",
            "Hydro"
          ],
          "labels": [
            "Bagasse",
            "Biogas",
            "Wind",
            "Hydro"
          ],
          "name": "Type of Fuel: "
        }
      }
    ],
    "transform": [
      {"filter": "datum.Fuel == Fuel_selection"}
    ],
    "mark": {
      "type": "line",
      "tooltip": true,
      "point": true
    },
    "encoding": {
      "x": {"field": "Year", "type": "nominal"},
      "y": {"field": "PJ", "type": "quantitative"},
      "color": {"field": "States", "type": "nominal"}
    }
  }
  